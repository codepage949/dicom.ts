{
  "name": "dicom.ts",
  "version": "1.0.0",
  "description": "A small, fast, typescript DICOM renderer",
  "author": "wearemothership",
  "keywords": [
    "dicom",
    "parser",
    "renderer",
    "canvas",
    "typescript",
    "glsl",
    "image",
    "jpeg",
    "jpeg2000",
    "jpeglossless",
    "jpegls",
    "RLE"
  ],
  "email": "nick@wearemothership.com",
  "license": "MIT",
  "repository": "wearemothership/dicom.ts",
  "homepage": "https://wearemothership.github.io/dicom.ts",
  "main": "dist/index.umd.js",
  "module": "dist/index.modern.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "exports": {
    "require": "./dist/index.umd.js",
    "default": "./dist/index.modern.js"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "rimraf dist && microbundle-crl --no-compress -f modern,umd",
    "start": "rimraf dist && microbundle-crl watch --no-compress -f modern,umd",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "jest .",
    "test:coverage": "jest . --coverage",
    "predeploy": "cd example-vs-cornerstone && npm install && SKIP_PREFLIGHT_CHECK=true npm run build",
    "deploy": "gh-pages -d example-vs-cornerstone/build"
  },
  "dependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@wearemothership/dicom-character-set": "^1.0.4-opt.1",
    "crypto-js": "^4.0.0",
    "jpeg-lossless-decoder-js": "git+https://github.com/rii-mango/JPEGLosslessDecoderJS.git#v2.0.4",
    "pako": "^1.0.10",
    "raw.macro": "^0.4.2",
    "twgl.js": "git+https://github.com/wearemothership/twgl.js#v4.19.1-node-canvas-webgl",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.13.8",
    "@babel/plugin-proposal-optional-chaining": "^7.13.8",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@types/crypto-js": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^27.2.4",
    "babel-plugin-macros": "^3.0.1",
    "blob-util": "^2.0.2",
    "canvas": "^2.8.0",
    "cross-env": "^7.0.2",
    "dicom-test-files": "git+https://github.com/wearemothership/dicom-test-files.git",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "gh-pages": "^2.2.0",
    "jest": "^27.2.4",
    "microbundle-crl": "^0.13.10",
    "node-canvas-webgl": "git+https://github.com/wearemothership/node-canvas-webgl.git#0.2.6-twgl",
    "node-fetch": "^3.0.0-beta.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "example",
      "example-vs-cornerstone"
    ]
  },
  "files": [
    "dist"
  ]
}
